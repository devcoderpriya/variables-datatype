#include <stdio.h>
 
int main()
{
    int num, count = 1;
 
    printf("Enter the Number for finding table:");
    scanf("%d", &num);
    printf("Multiplication table of %d are:\n ", num);
    while (count <= 10)
    {
        printf(" %d x %d = %d \n ", num, count, num * count);
        count++;
    }
    return 0;
}
